{
  "$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 4,
  "generator-cli": {
    "version": "5.3.1",
    "generators": {
      "angular": {
        "generatorName": "typescript-angular",
        "output": "./api-client/generated-sources/shop-app-api",
        "glob": "../../api/shop.yaml",
        "additionalProperties": {
          "ngVersion": "13.3.0",
          "npmName": "shop-app-api",
          "supportsES6": "true",
          "npmVersion": "7.6.1",
          "withInterfaces": true,
          "enumPropertyNaming": "UPPERCASE"
        }
      }
    }
  }
}
